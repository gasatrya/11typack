{
  "name": "11typack",
  "version": "1.0.0",
  "description": "Eleventy + webpack starter template",
  "scripts": {
    "dev:eleventy": "eleventy --serve --incremental",
    "dev:webpack": "webpack --config webpack/webpack.config.dev.js --watch",
    "dev": "cross-env ELEVENTY_ENV=development run-p clean dev:*",
    "build:eleventy": "eleventy",
    "build:webpack": "webpack --config webpack/webpack.config.prod.js",
    "build": "cross-env ELEVENTY_ENV=production run-s build:*",
    "clean": "rimraf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gasatrya/11typack.git"
  },
  "keywords": [
    "starter",
    "11ty",
    "webpack",
    "bootstrap"
  ],
  "author": "Ga Satrya",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.16",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.19.4",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eleventy-plugin-purgecss": "^0.3.0",
    "eslint": "^8.26.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.0.4",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-loader": "^7.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bootstrap": "^5.2.2",
    "core-js": "^3.26.0"
  }
}
